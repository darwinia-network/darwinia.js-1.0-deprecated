{
  "name": "darwinia.js",
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@polkadot|@babel/runtime/helpers/esm/)"
    ]
  },
  "private": true,
  "repository": "https://github.com/darwinia-network/darwinia.js",
  "license": "Apache-2",
  "workspaces": [
    "packages/*"
  ],
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0",
    "typescript": "4.3.3"
  },
  "scripts": {
    "build": "yarn build:interfaces && yarn build:ts",
    "build:interfaces": "darwinia-types-internal-interfaces",
    "publish": "darwinia-ci-ghact-build",
    "build:ts": "darwinia-dev-build-ts",
    "docs": "polkadot-types-internal-metadata && polkadot-dev-build-docs",
    "docs:release": "polkadot-ci-ghact-docs",
    "chain:info": "polkadot-types-chain-info",
    "lint": "polkadot-dev-run-lint",
    "clean": "polkadot-dev-clean-build",
    "postinstall": "polkadot-dev-yarn-only",
    "test": "polkadot-dev-run-test --coverage --runInBand --testPathIgnorePatterns e2e",
    "test:one": "polkadot-dev-run-test --detectOpenHandles --forceExit",
    "test:watch": "polkadot-dev-run-test --watch",
    "lernaversion": "lerna version --skip-git",
    "build-types-bundle": "node -r ts-node/register -r tsconfig-paths/register packages/types/scripts/build-json.ts"
  },
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.13.10",
    "@babel/register": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@darwinia/typegen": "workspace:packages/typegen",
    "@polkadot/api": "8.1.2-1",
    "@polkadot/dev": "^0.55.46",
    "@polkadot/ts": "^0.3.42",
    "@polkadot/typegen": "8.1.2-1",
    "@types/jest": "^26.0.13",
    "@vuepress/plugin-search": "^1.5.4",
    "copyfiles": "^2.3.0",
    "cpx": "^1.5.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.19.1",
    "typedoc-plugin-markdown": "^2.4.2",
    "typedoc-plugin-no-inherit": "^1.1.10"
  },
  "version": "2.7.6"
}
